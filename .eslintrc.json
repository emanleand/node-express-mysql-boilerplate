{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "airbnb-base"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["error", 2],
        "comma-dangle": "off",
        "no-console": "off",
        "max-len": [
          "error",
          {
            "code": 110,
            "tabWidth": 2,
            "ignoreStrings": true,
            "ignoreTemplateLiterals": true
          }
        ]
    }
}
